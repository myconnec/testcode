{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CloudWatchLoggin",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "logs:DescribeLogStreams"
            ],
            "Resource": [
                "arn:aws:logs:*:*:*"
            ]
        },
        {
            "Sid": "CodeCommitReadOnly",
            "Effect": "Allow",
            "Action": [
                "codecommit:Get*",
                "codecommit:GitPull",
                "codecommit:BatchGetRepositories"
            ],
            "Resource": [
                "arn:aws:codecommit:us-east-1:345292015349:connechub*"
            ]
        }
    ]
}