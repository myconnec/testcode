<div class="col-xs-6 col-sm-4 col-md-3">
  <%= render :partial => 'categories/search' %>
  <%= render :partial => 'partials/side_menu', locals: {
    campus: @categories[0],
    community: @categories[1],
    free: @categories[4],
    h2f: @categories[3],
    housing: @categories[2],
    jobs: @categories[5],
    sale: @categories[6],
    services: @categories[7],
    unboxing: @categories[8],
  } %>
</div>

<div class="col-xs-6 col-sm-8 col-md-9">
  <div class="grid">
    <% if @listings.present? %>
      <% @listings.each do |listing| %>
        <%= render :partial => 'partials/item_card', locals: {
          listing: listing
        } %>
      <% end %>
    <% else %>
      <p>No listings found!</>
    <% end %>
  </div>
</div>
